puts Hello

Assert! we are on
Assert! when we are on {{} {
    When the counter is /t/ {
        puts $t
        Claim the counter is running
    }
    When the counter is running {
        puts "[__threadId]: RERUNNING"
    }

    When we are on {
        puts "RUNNING HOLDS"
        for {set i 0} {$i < 200} {incr i} {
            Hold! counter [list X claims the counter is $i]
        }
        puts DONE
    }
}} with environment {}
