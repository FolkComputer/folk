set cc [C]
$cc cflags -I.
$cc include "cache.h"

$cc proc test {} void {
    Cache *c = cacheNew(interp);

    Jim_Obj *hello = cacheGetOrInsert(c, interp, "hello");
    printf("Hello: %p\n", hello);
    Jim_Obj *hello2 = cacheGetOrInsert(c, interp, "hello");
    printf("Hello2: %p\n", hello2);
    FOLK_ENSURE(hello == hello2);
}

[$cc compile] test
__exit 0
