set radiox -815.2; set radioy 1629.4

set bandsize 487

When the RFID tag has phase /radians/ {
	# MyCurveFit.com
	set baseradius [expr {675.368 + -83.1298*$radians}]
	for {set i 0} {$i < 5} {incr i} {
		set radius [expr {$baseradius+$bandsize*$i}]
		Wish display runs [list Display::circle $radiox $radioy $radius 5 green]
	}
}
